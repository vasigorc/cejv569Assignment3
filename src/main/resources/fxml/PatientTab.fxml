<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="AnchorPane" fx:id="patientTab" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ca.vgorcinschi.controller.PatientTabController">
    <stylesheets>
        <URL value="@/styles/patienttab.css" />
    </stylesheets>
   <children>
      <HBox prefHeight="400.0" prefWidth="600.0">
         <children>
            <TableView fx:id="patientDataTable" prefHeight="400.0" prefWidth="600.0">
              <columns>
                <TableColumn fx:id="idColumn" prefWidth="46.0" text="ID" />
                <TableColumn fx:id="lastNameColumn" prefWidth="127.0" text="Last Name" />
                  <TableColumn fx:id="firstNameColumn" prefWidth="94.0" text="First Name" />
                  <TableColumn fx:id="diagnosisColumn" prefWidth="84.0" text="Diagnosis" />
                  <TableColumn fx:id="admissionDateColumn" prefWidth="129.0" text="Admission Date" />
                  <TableColumn fx:id="releaseDateColumn" prefWidth="119.0" text="Release Date" />
              </columns>
            </TableView>
         </children>
      </HBox>
   </children>
</AnchorPane>