<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" 
            prefWidth="600.0" styleClass="mainFxmlClass" 
            xmlns:fx="http://javafx.com/fxml/1" 
            fx:controller="ca.vgorcinschi.controller.InpatientTabController">
    <stylesheets>
        <URL value="@/styles/inpatienttab.css"/>
    </stylesheets>
    <children>
        <VBox prefHeight="700.0" prefWidth="900.0">
            <children>
                <TableView fx:id="inpatientDataTable" prefHeight="280.0" prefWidth="900.0">
                    <columns>
                        <TableColumn fx:id="idColumn" prefWidth="52.0" text="ID" />
                        <TableColumn fx:id="dateOfMedicationColumn" prefWidth="170.0" text="%dateOfMedication" />
                        <TableColumn fx:id="medNameColumn" prefWidth="212.0" text="%medName" />
                        <TableColumn fx:id="unitCostColumn" prefWidth="212.0" text="%unitCost" />
                        <TableColumn fx:id="unitsColumn" prefWidth="212.0" text="%nrUnits" />
                    </columns>
                </TableView>
            </children>
        </VBox>
    </children>
</AnchorPane>
